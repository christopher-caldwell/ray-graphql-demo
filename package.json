    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "pg-to-ts": "^4.1.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@caldwell619/data-loader": "0.0.9-alpha.0",
    "@pothos/core": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
